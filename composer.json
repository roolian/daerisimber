{
  "name": "daeris/daerisimber",
  "type": "wordpress-theme",
  "description": "A modern WordPress theme using Timber, ACF, Tailwind CSS and Alpine JS",
  "license": "MIT",
  "authors": [{
    "name": "Daeris",
    "email": "contact@daeris.fr"
  }],
  "require": {
    "php": ">=8.1",
    "hassankhan/config": "^3.1",
    "hellonico/timber-dump-extension": "^2.0",
    "phlak/config": "^8.0",
    "timber/timber": "^2.0"
  },
  "config": {
    "optimize-autoloader": true,
    "sort-packages": true,
    "allow-plugins": {
      "composer/installers": true
    }
  },
  "minimum-stability": "stable",
  "autoload": {
      "psr-4": {
          "Daerisimber\\": "theme/src/",
          "Daerisimber\\Modules\\": "theme/modules/"
      }
  }
}
